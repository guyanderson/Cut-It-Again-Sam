@{
  Layout="Shared/_Layout.cshtml";
}


<h1>All Stylists</h1>
@if (@Model.Count == 0)
{
  <p>You have no body working here!</p>
}
<ul>
  @foreach (var stylist in Model)
  {
    <li><a href="/stylist/@stylist.GetId()">@stylist.GetName()</a></li>
  }
</ul>
<a href="/stylist/new">Add a new stylist to your team.</a>
